{
  "name": "Google Meet Lights On",
  "version": "1.0",
  "description": "Switch on you Elgato Key Lights/Philps Hue when you join a meet session",
  "manifest_version": 3,
  "host_permissions": ["https://*/*", "http://*/*"],
  "permissions": ["scripting", "activeTab", "tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
    "page": "option.html",
    "open_in_tab": true
  }
}
